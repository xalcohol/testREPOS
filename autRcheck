local UIS = game:GetService('UserInputService')
local cas = game:GetService("ContextActionService")
local autR = true

function onB()
    print("Disabling")
        task.wait(1)
        autR = false
    task.wait(1)
    print("Disabled")
        task.wait(1)
        loadstring(game:HttpGet("https://raw.githubusercontent.com/xalcohol/testREPOS/refs/heads/main/relaunch"))()
    task.wait(1)
    print("Reloaded")
end

cas:BindAction("B", onB, false, Enum.KeyCode.B)

UIS.InputBegan:Connect(function(input)
    if input.KeyCode == Enum.KeyCode.B then
        cas:UnbindAction("B")
end)

while autR == true do 
    wait(2)
      game:GetService("ReplicatedStorage"):WaitForChild("Universe"):WaitForChild("Network"):WaitForChild("Remotes"):WaitForChild("Rebirth"):FireServer()
end
